{
    "type": "symfony-bundle",
    "license": "MIT",
    "name": "akmaks/command-chaining-bundle",
    "description": "Bundle provides some functional for calling console commands in chain",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.2.5",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "symfony/console": "5.2.*",
        "symfony/dotenv": "5.2.*",
        "symfony/expression-language": "5.2.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "5.2.*",
        "symfony/monolog-bundle": "^3.7",
        "symfony/yaml": "5.2.*",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {"Akmaks\\Bundle\\CommandChainingBundle\\": ""},
        "exclude-from-classmap": ["/Tests/"]
    }
}
